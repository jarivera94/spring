	<queryString>
		<![CDATA[SELECT a.avaluo_id, dea.id id_descripcion_area, a.valor_total_avaluo valor_razonable, a.valor_avaluo_en_uvr, a.valor_uvr ,
    a.valor_asegurable, cal.nombre calificacion_garantia, h.valor_proporcional_terreno,
    h.valor_proporcional_construccion, h.valor_integral_terreno, h.valor_integral_construccion,
    h.metodo_de_valuacion, h.actualidad_edificadora, h.comportamiento_oferta_demanda,
    h.tiempo_esperado_de_comercializacion, observaciones, COALESCE(h.direccion_anexos||'. ','')||COALESCE(h.otras_direcciones,'') direccion_anexos_y_otras,
    us.nombres, us.apellidos, us.registro, aa.valor_reposicion_final, dea.nombre descripcion, aa.nombre,
    aa.tamano_area, h.porcentaje_terreno,
    CASE WHEN a.valor_total_avaluo > 0 THEN h.valor_proporcional_terreno / a.valor_total_avaluo ELSE 0 END porcentaje_valor_proporcional_terreno,
    CASE WHEN a.valor_total_avaluo > 0 THEN h.valor_proporcional_construccion / a.valor_total_avaluo ELSE 0 END porcentaje_valor_proporcional_construccion,
    ea.estado, h.descripcion_general, h.direccion_anexos, h.tfc1, h.tfc2, h.tfc3, h.tfc4, h.tfc5, h.tfc6, h.tfc7, h.tfc8, h.tfc9, h.tfc10,
(SELECT SUM(ar.tamano_area) area_total_construccion
FROM avaluos.area ar
WHERE ar.descripcion_numerica IN ('1','2','22')
AND ar.avaluo_id = a.avaluo_id),
(SELECT SUM(ar.tamano_area) area_total_terreno
FROM avaluos.area ar
WHERE ar.descripcion_numerica = '21'
AND ar.avaluo_id = a.avaluo_id),
(SELECT SUM(ar.porcentaje) total_porcentajes
FROM avaluos.area ar
WHERE ar.avaluo_id = a.avaluo_id)
FROM
    avaluos.avaluo a
    JOIN avaluos.formato_informe f ON (a.avaluo_id = f.avaluo_id)
    JOIN avaluos.formato_informe_hipotecario h ON (f.formato_informe_id = h.formato_informe_id)
    JOIN avaluos.calificacion_garantia cal ON (a.calificacion_garantia = cal.id)
    JOIN avaluos.usuario us ON (a.perito_tipo_documento = us.tipo_documento_identificacion AND a.perito_numero_documento = us.numero_documento)
    JOIN avaluos.area aa ON (aa.avaluo_id = a.avaluo_id)
    LEFT JOIN avaluos.descripcion_area dea ON (aa.descripcion_numerica = dea.id /*AND dea.id IN(1,22)*/)
    JOIN avaluos.estado_avaluo ea ON (ea.avaluo_id = a.avaluo_id)
WHERE ea.estado_actual = TRUE

AND a.avaluo_id = $P{IDAVALUO}
ORDER BY aa.nombre]]>
	</queryString>
